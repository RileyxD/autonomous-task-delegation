{
  "name": "claude-bash",
  "enabled": true,
  "description": "Claude Bash subagent for terminal and git-heavy execution.",
  "command": "claude",
  "promptMode": "argument",
  "defaultArgs": [
    "--model",
    "qwen3-coder-capped",
    "--agent",
    "Bash",
    "--permission-mode",
    "bypassPermissions",
    "-p"
  ],
  "useWorktree": true
}
